{
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".css": "text/css",
    ".map": "application/json",
    ".json": "application/json"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.mjs",
      "/favicon.ico",
      "/robots.txt",
      "/manifest.webmanifest"
    ]
  },
  "routes": [
    { "route": "/assets/*", "allowedRoles": ["anonymous"] }
  ]
}